{
  "name": "kua",
  "version": "1.0.21",
  "description": "",
  "main": "index.js",
  "scripts": {
    "lint": "eslint *.js src/**/*.js",
    "test": "jest",
    "test:watch": "npm run test -- --watch",
    "build": "babel src --out-dir lib",
    "build:watch": "chokidar 'src/**/*.js' -c 'npm run build'"
  },
  "directories": {
    "bin": "./bin"
  },
  "files": [
    "lib",
    "bin",
    "index.js"
  ],
  "babel": {
    "presets": [
      "env",
      "stage-2"
    ]
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": [
      "standard",
      "plugin:react/recommended",
      "plugin:import/errors",
      "plugin:import/warnings"
    ],
    "env": {
      "jest": true
    },
    "rules": {
      "space-before-function-paren": "off",
      "comma-dangle": [
        2,
        "always-multiline"
      ],
      "object-curly-spacing": [
        2,
        "always"
      ],
      "array-bracket-spacing": [
        2,
        "never"
      ],
      "import/no-unresolved": [
        2,
        {
          "ignore": [
            "^config$"
          ]
        }
      ],
      "import/order": [
        2,
        {
          "newlines-between": "always"
        }
      ],
      "jsx-quotes": [
        2,
        "prefer-double"
      ]
    }
  },
  "dependencies": {
    "babel-cli": "^6.16.0",
    "babel-eslint": "^7.0.0",
    "babel-polyfill": "^6.16.0",
    "babel-preset-env": "^1.2.1",
    "babel-preset-stage-2": "^6.17.0",
    "babel-register": "^6.16.3",
    "bluebird": "^3.4.6",
    "chokidar": "^1.6.0",
    "chokidar-cli": "^1.2.0",
    "daemonize2": "^0.4.2",
    "deep-extend": "^0.4.1",
    "eslint": "^3.7.0",
    "eslint-config-standard": "^7.0.1",
    "eslint-plugin-import": "^2.0.0",
    "eslint-plugin-promise": "^3.4.1",
    "eslint-plugin-react": "^6.3.0",
    "eslint-plugin-standard": "^2.0.1",
    "glob": "^7.1.0",
    "inflection": "^1.10.0",
    "jest-cli": "^19.0.2",
    "js-yaml": "^3.6.1",
    "object-path": "^0.11.2",
    "optimist": "^0.6.1",
    "ramda": "^0.23.0",
    "uuid": "^3.0.1"
  }
}
